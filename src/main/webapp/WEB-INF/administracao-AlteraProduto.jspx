<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:fmt="http://java.sun.com/jsp/jstl/fmt" version="2.0">

    <jsp:directive.page contentType="text/html" pageEncoding="UTF-8"/>
    <jsp:output omit-xml-declaration="true" doctype-root-element="HTML" doctype-system="about:legacy-compact"/>

    <html>
        <head>
            <title>Posto - Administração</title>
            <link rel="stylesheet" type="text/css" href="css/administracao-AlteraProduto.css"/>
            <link rel="stylesheet" type="text/css" href="css/geral.css"/>
        </head>

        <body>

            <div id="nav">
                <ul id="menu">
                    <li><a href="NamtabServlet">Home</a></li>
                    <li><a href="RelatorioServlet">Relatório</a></li>
                    <li>Administração
                        <ul> 
                            <li><a href="AdministracaoAlteraProdutoServlet">Produtos</a></li>
                            <li><a href="AdministracaoServlet">Usuários</a></li>
                            <li><a href="AdministracaoAlteraSenhaServlet">Alterar senha</a></li>
                        </ul>
                    </li>
                    <li><a href="VendaServlet">Venda</a></li>
                    <p id="varUsuario">Welcome, <c:out value="${usuario}"/></p>

                </ul>
            </div>
            <form id="formulario" action="AdministracaoAlteraProdutoServlet" method="get">
                <fieldset>
                    <legend>Produto</legend>
                    <label class="labelformulario" id="lblcargo">Produto: </label> 
                    <select name="usuario" >
                        <option>Gerente</option>
                        <option>Frentista</option>
                        <option>Vendedor</option>

                    </select>
                    <label class="labelformulario">Preço: </label> <input type="text" name="senha"> </input><br></br>


                </fieldset>

                <fieldset>
                    <legend>Opções</legend>
                    <button type="submit" class="btn">Alterar</button>
                </fieldset>
            </form>



            <span>Matriz - Av Deputado Oswaldo Moraes e silva</span>

        </body>

    </html>

